function load() {
  var listElement = document.getElementById("xpromo_list");
  var popup = document.getElementById("xpromo_popup");
  if(listElement.addEventListener) {
    listElement.addEventListener("mouseover", showXPromoPopup, false);
    listElement.addEventListener("mouseout", hideXPromoPopup, false);
  } else if (listElement.attachEvent){
    listElement.attachEvent("onmouseover", showXPromoPopup);
    listElement.attachEvent("onmouseout", hideXPromoPopup);
  }
  if(popup.addEventListener) {
    popup.addEventListener("mouseover", showXPromoPopup, false);
    popup.addEventListener("mouseout", hideXPromoPopup, false);
  } else if (popup.attachEvent){
    popup.attachEvent("onmouseover", showXPromoPopup);
    popup.attachEvent("onmouseout", hideXPromoPopup);
  }
}
function showXPromoPopup() {
  var popup = document.getElementById("xpromo_popup");
  if(popup.className) {
    popup.className = popup.className + " hover";
  } else {
    popup.setAttribute("className", popup.getAttribute("className") + " hover");
  }
  popup.style.display = 'block';
}
function hideXPromoPopup() {
  var popup = document.getElementById("xpromo_popup");
  if(popup.className) {
    popup.className = popup.className.replace(" hover", "");  
  } else {
    popup.setAttribute("className", popup.getAttribute("className").replace(" hover", ""));
  }

  popup.style.display = 'none';
}

